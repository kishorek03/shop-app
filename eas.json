{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "qa": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "qa"
      },
      "channel": "qa"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "production"
      },
      "channel": "production"
    }
  }
} 